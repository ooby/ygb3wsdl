<?xml version='1.0' encoding='UTF-8'?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://www.hostco.ru/types" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.hostco.ru/types">
    <xs:simpleType name="GUIDType">
        <xs:annotation>
            <xs:documentation>GUID</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="64"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SNILSType">
        <xs:annotation>
            <xs:documentation>СНИЛС</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="11"/>
            <xs:maxLength value="11"/>
            <xs:pattern value="[0-9]{11}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="KLADRCodeStreetType">
        <xs:annotation>
            <xs:documentation>Код КЛАДР до улицы</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="17"/>
            <xs:pattern value="\d{17}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="KLADRCodeType">
        <xs:annotation>
            <xs:documentation>Код КЛАДР до улицы</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="13"/>
            <xs:pattern value="\d{13}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SlipNumberType">
        <xs:annotation>
            <xs:documentation>Номер талона</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RequestHouseNumber">
        <xs:annotation>
            <xs:documentation>Номера домов для запроса поиска участков</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d+"/>
            <xs:pattern value="\d+\p{L}"/>
            <xs:pattern value="\d+\p{L}?/\d+"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="HouseNumberRange">
        <xs:annotation>
            <xs:documentation>Диапазон номеров домов</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d+-\d+"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="HouseNumber">
        <xs:annotation>
            <xs:documentation>Номера домов</xs:documentation>
        </xs:annotation>
        <xs:union memberTypes="ns1:RequestHouseNumber ns1:HouseNumberRange"/>
    </xs:simpleType>
    <xs:simpleType name="RoomNumberType">
        <xs:annotation>
            <xs:documentation>Номер кабинета</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="100"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="MuCodeType">
        <xs:annotation>
            <xs:documentation>Код ЛПУ</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="12"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ProfCodeType">
        <xs:annotation>
            <xs:documentation>Внешний код проффесионала</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="PosCodeType">
        <xs:annotation>
            <xs:documentation>Код должности специалиста</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="DeptCodeType">
        <xs:annotation>
            <xs:documentation>Код отделения</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AppointmentSourceType">
        <xs:annotation>
            <xs:documentation>Источник записи
                (1-ЛПУ,
                2-Портал,
                3-Терминал,
                4-Мобильное приложение,
                5-Федеральная регистратура,
                6-Региональный ПГУ)
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="AppointmentStatusType">
        <xs:annotation>
            <xs:documentation>Статус записи на прием (0-Фиктивный (отсутствует запись на
                приём),1-Активная, 2-Выполнена,
                3-Отменена, 4-Не выполнена (пациент не явился), 5-Не выполнена (врач на больничном))
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:enumeration value="0"/>
            <xs:enumeration value="1"/>
            <xs:enumeration value="2"/>
            <xs:enumeration value="3"/>
            <xs:enumeration value="4"/>
            <xs:enumeration value="5"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="FilterOptionsType">
        <xs:annotation>
            <xs:documentation>Набор ключевых атрибутов одного элемента расписания, для фильтрации
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="scheduleDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>дата расписания</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="muCode" type="ns1:MuCodeType">
                <xs:annotation>
                    <xs:documentation>код ЛПУ в системе ОФОМС</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="deptCode" type="ns1:DeptCodeType">
                <xs:annotation>
                    <xs:documentation>Код отделения внутри ЛПУ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="roomNumber" type="ns1:RoomNumberType">
                <xs:annotation>
                    <xs:documentation>номер кабинета</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice minOccurs="0">
                <xs:sequence>
                    <xs:element minOccurs="0" name="docCode" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>код медработника</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="specCode" type="xs:long">
                        <xs:annotation>
                            <xs:documentation>код одной из специальностей медработника
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="positionCode" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>код одной из должностей медработника
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="scheduleInfo" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Дополнительная информация</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="docFIO" type="xs:string"/>
                    <xs:element minOccurs="0" name="docSNILS" type="ns1:SNILSType">
                        <xs:annotation>
                            <xs:documentation>СНИЛС врача</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:element name="actionCode" type="xs:long">
                    <xs:annotation>
                        <xs:documentation>код медицинского действия</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
            <xs:element minOccurs="0" name="needFIO" type="xs:boolean"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ScheduleElementType">
        <xs:annotation>
            <xs:documentation>Набор ключевых атрибутов одного элемента расписания, для
                создания/удаления расписания
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="ns1:FilterOptionsType">
                <xs:sequence>
                    <xs:element name="scheduleDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>дата расписания</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="muCode" type="ns1:MuCodeType">
                        <xs:annotation>
                            <xs:documentation>код ЛПУ в системе ОФОМС</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="deptCode" type="ns1:DeptCodeType">
                        <xs:annotation>
                            <xs:documentation>Код отделения внутри ЛПУ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="roomNumber" type="ns1:RoomNumberType">
                        <xs:annotation>
                            <xs:documentation>номер кабинета</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:choice>
                        <xs:sequence>
                            <xs:element name="docCode" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>код медработника</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="specCode" type="xs:long">
                                <xs:annotation>
                                    <xs:documentation>код одной из специальностей медработника
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="positionCode" type="xs:int">
                                <xs:annotation>
                                    <xs:documentation>код одной из должностей медработника
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="scheduleInfo" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Дополнительная информация</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="docFIO" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>ФИО врача</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="docSNILS" type="ns1:SNILSType">
                                <xs:annotation>
                                    <xs:documentation>СНИЛС врача</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                        <xs:element name="actionCode" type="xs:long">
                            <xs:annotation>
                                <xs:documentation>код медицинского действия</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:choice>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="AppointmentType">
        <xs:annotation>
            <xs:documentation>Информация о записи на приём</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="slipNumber">
                <xs:annotation>
                    <xs:documentation>Номер талона</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="appointmentSource" type="ns1:AppointmentSourceType">
                <xs:annotation>
                    <xs:documentation>Источник записи
                        1 ЛПУ
                        2 Портал
                        3 Терминал
                        4 Мобильное приложение
                        5 Федеральная регистратура
                        6 Региональный ПГУ
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="appointmentStatus" type="ns1:AppointmentStatusType">
                <xs:annotation>
                    <xs:documentation>Статус записи на приём</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="patientFIO" type="xs:string">
                <xs:annotation>
                    <xs:documentation>ФИО записанного пациента</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="PeriodType">
        <xs:annotation>
            <xs:documentation>Период по дате</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="beginDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Начальная дата</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="endDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Конечная дата</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TimeIntervalType">
        <xs:annotation>
            <xs:documentation>Интервал времени</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="timeStart" type="xs:time">
                <xs:annotation>
                    <xs:documentation>Время начала</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="timeFinish" type="xs:time">
                <xs:annotation>
                    <xs:documentation>Время окончания</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="DepartmentType">
        <xs:annotation>
            <xs:documentation>Информация об отделении ЛПУ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="deptCode" type="ns1:DeptCodeType">
                <xs:annotation>
                    <xs:documentation>код отделения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="deptName">
                <xs:annotation>
                    <xs:documentation>название отделения</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="250"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="deptShortName">
                <xs:annotation>
                    <xs:documentation>сокращенное название отделения</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="30"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="deptType" type="xs:int">
                <xs:annotation>
                    <xs:documentation>код типа отделения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="deptAddress">
                <xs:annotation>
                    <xs:documentation>адрес отделения</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="200"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="deptPhone">
                <xs:annotation>
                    <xs:documentation>телефон отделения</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="deleted" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Признак удаления</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="positions" type="ns1:PositionDeptType">
                <xs:annotation>
                    <xs:documentation>Должности в отделении</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ExtendedDepartmentType">
        <xs:annotation>
            <xs:documentation>Информация об отделении с кабинетами и работниками</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="ns1:DepartmentType">
                <xs:sequence>
                    <xs:sequence maxOccurs="unbounded" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Список кабинетов</xs:documentation>
                        </xs:annotation>
                        <xs:element name="room" type="ns1:RoomType"/>
                    </xs:sequence>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="RoomType">
        <xs:annotation>
            <xs:documentation>Информация о кабинете</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="roomNumber" type="ns1:RoomNumberType">
                <xs:annotation>
                    <xs:documentation>Номер кабинета</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="deleted" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Признак удаления</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="PatientInfoType">
        <xs:annotation>
            <xs:documentation>Информация о пациенте (все возможные идентификаторы)
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="Lastname">
                <xs:annotation>
                    <xs:documentation>Фамилия</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="40"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="Firstname">
                <xs:annotation>
                    <xs:documentation>Имя</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="40"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="Middlename">
                <xs:annotation>
                    <xs:documentation>Отчество</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="40"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="birthDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата рождения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="policyNumber">
                <xs:annotation>
                    <xs:documentation>Полис ОМС</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="16"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="policyNumberDMS">
                <xs:annotation>
                    <xs:documentation>Полис ДМС</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="32"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="SNILS" type="ns1:SNILSType">
                <xs:annotation>
                    <xs:documentation>Номер СНИЛС</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="passportSeries">
                <xs:annotation>
                    <xs:documentation>Серия паспорта</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="4"/>
                        <xs:pattern value="\d{4}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="passportNumber">
                <xs:annotation>
                    <xs:documentation>Номер паспорта</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="6"/>
                        <xs:pattern value="\d{6}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="phone">
                <xs:annotation>
                    <xs:documentation>Телефон</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="32"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="email">
                <xs:annotation>
                    <xs:documentation>Электронная почта</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="64"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="StaffInfoType">
        <xs:annotation>
            <xs:documentation>Информация о работнике ЛПУ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="docCode">
                <xs:annotation>
                    <xs:documentation>Код специалиста</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="snils">
                <xs:annotation>
                    <xs:documentation>СНИЛС</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:length value="11"/>
                        <xs:pattern value="([0-9]{11})"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="firstName">
                <xs:annotation>
                    <xs:documentation>Имя</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="30"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="middleName">
                <xs:annotation>
                    <xs:documentation>Отчество</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="30"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="lastName">
                <xs:annotation>
                    <xs:documentation>Фамилия</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="30"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="specialist_info">
                <xs:annotation>
                    <xs:documentation>Дополнительные сведения о специалисте</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:sequence maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Список специальностей</xs:documentation>
                </xs:annotation>
                <xs:element name="specCode" type="xs:int">
                    <xs:annotation>
